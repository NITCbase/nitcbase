echo TEST: DDL Commands and Insert Commands
echo OPEN TABLE
OPEN TABLE testTable
echo _____________________________________________________________________________________________________________________
echo CLOSE TABLE
CLOSE TABLE testTable
echo _____________________________________________________________________________________________________________________
echo CREATE TABLE
CREATE TABLE Student(Name STR, Age NUM, Gender STR)
echo _____________________________________________________________________________________________________________________
echo DROP TABLE
DROP TABLE Student
echo _____________________________________________________________________________________________________________________
echo CREATE INDEX
CREATE INDEX ON Student.Rollno
echo _____________________________________________________________________________________________________________________
echo DROP INDEX
DROP INDEX ON Student.Rollno
echo _____________________________________________________________________________________________________________________
echo ALTER TABLE RENAME
ALTER TABLE RENAME Student TO STUDENTS
echo _____________________________________________________________________________________________________________________
echo ALTER TABLE RENAME COLUMN
ALTER TABLE RENAME Student COLUMN CGPA TO SGPA
echo _____________________________________________________________________________________________________________________
echo INSERT INTO VALUES
INSERT INTO Student VALUES (Isa, 16, Female);
echo _____________________________________________________________________________________________________________________
echo INSERT VALUES FROM FILE
INSERT INTO Student VALUES FROM 123.csv;
echo _____________________________________________________________________________________________________________________