echo creating a new relation Student_Grades
CREATE TABLE Student_Grades(ID NUM, FirstName STR, Grade STR);
echo inserting few records
OPEN TABLE Student_Grades;
INSERT INTO Student_Grades VALUES (1, Isa, S);
INSERT INTO Student_Grades VALUES (2, Lynne, A);
