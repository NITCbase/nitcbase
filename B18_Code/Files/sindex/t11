fdisk
import 2000nums.csv
OPEN TABLE 2000nums
CREATE INDEX ON 2000nums.key
echo Selecting Grades greater than or equal to 200 after indexing
SELECT * FROM 2000nums INTO 2000nums_1 WHERE key >= 200;
export 2000nums_1 2000nums_1.csv
echo Selecting Grades equal to 200 after indexing
SELECT * FROM 2000nums INTO 2000nums_2 WHERE key = 200;
export 2000nums_2 2000nums_2.csv