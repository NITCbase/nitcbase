*** renaming to attr name which already exists
fdisk
create table a(id NUM, roll NUM)
alter table rename a column id to roll
exit

*** renaming attr which does not exist
fdisk
create table a(id NUM)
alter table rename a column identifier to rollno
exit

*** renaming RELATIONCAT / ATTRIBUTECAT
alter table rename ATTRIBUTECAT column id to rollno
alter table rename RELATIONCAT column id to rollno

*** renaming when relation does not exist
fdisk
alter table rename a column id to rollno
exit

*** renaming when relation open
fdisk
create table a(id NUM)
open table a
alter table rename a column id to rollno
exit